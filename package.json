{
	"name": "@onedotprojects/auth",
	"description": "All-In-One Authorization",
	"author": "OneDot",
	"keywords": [
		"auth",
		"login",
		"signup",
		"signin",
		"authorization"
	],
	"homepage": "https://auth.onedot.cf",
	"private": true,
	"license": "MIT",
	"version": "1.0.0",
	"engines": {
		"node": "14.16.1"
	},
	"bugs": {
		"url": "https://github.com/onedotprojects/auth/issues/new",
		"email": "1auth.wgyt@gmail.com"
	},
	"main": "src/index.js",
	"scripts": {
		"lint": "npx prettier --write . && npx eslint --fix . & npx jshint .",
		"start": "TAILWIND_MODE=watch npx postcss src/tailwind.sass -o src/routes/main/style.css & node --trace-warnings --harmony src/index.js",
		"test": "npx nodemon --harmony --trace-warnings src/index.js"
	},
	"dependencies": {
		"@replit/database": "^2.0.1",
		"atob": "^2.1.2",
		"body-parser": "^1.19.0",
		"clean-css": "^5.1.2",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.5",
		"dotenv": "^8.5.1",
		"express": "^4.17.1",
		"fs": "0.0.1-security",
		"globby": "^11.0.3",
		"google-auth-library": "^7.0.4",
		"jsdom": "^16.5.3",
		"mustache": "^4.2.0",
		"mustache-express": "^1.3.0",
		"node-fetch": "^2.6.1",
		"nodemailer": "^6.6.0",
		"retronid": "^1.0.2"
	},
	"devDependencies": {
		"@tailwindcss/forms": "^0.3.2",
		"@tailwindcss/typography": "^0.4.0",
		"@types/body-parser": "^1.19.0",
		"@types/clean-css": "^4.2.4",
		"@types/compression": "^1.7.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.1",
		"@types/globby": "^9.1.0",
		"@types/jsdom": "^16.2.10",
		"@types/mustache": "^4.1.1",
		"@types/mustache-express": "^1.2.1",
		"@types/node": "14.14.44",
		"@types/node-fetch": "^2.5.10",
		"@types/nodemailer": "^6.4.1",
		"@types/postcss-import": "^12.0.0",
		"autoprefixer": "^10.2.5",
		"eslint": "^7.25.0",
		"eslint-plugin-html": "^6.1.2",
		"jshint": "^2.12.0",
		"nodemon": "^2.0.7",
		"postcss": "^8.2.13",
		"postcss-advanced-variables": "^3.0.1",
		"postcss-cli": "^8.3.1",
		"postcss-import": "^14.0.1",
		"postcss-map-get": "^0.3.0",
		"postcss-nested": "^5.0.5",
		"postcss-scss": "^3.0.5",
		"postcss-sort-media-queries": "^3.9.10",
		"prettier": "^2.2.1",
		"tailwindcss": "^2.1.2"
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"trailingComma": "all",
		"jsxBracketSameLine": true,
		"vueIndentScriptAndStyle": true,
		"embeddedLanguageFormatting": "auto",
		"printWidth": 100
	},
	"nodemonConfig": {
		"verbose": false,
		"ext": "*",
		"events": {
			"restart": "npx prettier --write $FILENAME"
		}
	},
	"browserslist": [
		"> 0%"
	]
}
